{
    "name": "smile/module-store-delivery",
    "type": "magento2-module",
    "description": "Smile Store Delivery",
    "keywords": ["magento2", "store delivery", "click in store", "delivery in store"],
    "license": "OSL-3.0",
    "authors": [
        {
            "name": "Romain Ruaud",
            "email": "romain.ruaud@smile.fr"
        }
    ],
    "require": {
        "php": "^8.1",
        "magento/framework": ">=103.0.4",
        "magento/module-checkout": ">=100.4.4",
        "magento/module-store": ">=101.1.4",
        "smile/module-store-locator": "dev-2.2.x"
    },
    "require-dev": {
        "smile/magento2-smilelab-quality-suite": "^3.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.magento.com/"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Smile\\StoreDelivery\\": ""
        }
    },
    "config": {
        "allow-plugins": {
            "magento/composer-dependency-version-audit-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        },
        "sort-packages": true
    }
}
